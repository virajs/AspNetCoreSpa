{
  "name": "aspnetnetcore",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "rimraf": "rimraf",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "typings": "typings",
    "webpack": "webpack",
    "webdriver-manager": "webdriver-manager",
    "protractor": "protractor",
    "coverage": "npm run coverage:remap && npm run coverage:report",
    "coverage:remap": "remap-istanbul -i coverage/coverage.json -o coverage/coverage.json -t json -e node_modules,tests,karma.entry.ts",
    "coverage:report": "istanbul report",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc typings coverage wwwroot/dist",
    "clean:dist": "npm run rimraf -- wwwroot/dist",
    "preclean:install": "npm run clean",
    "clean:install": "npm set progress=false && npm install",
    "preclean:start": "npm run clean",
    "clean:start": "npm start",
    "lint": "npm run tslint \"Client/**/*.ts\"",
    "pretest": "npm run lint",
    "test": "karma start",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "webdriver:update": "npm run webdriver-manager update",
    "webdriver:start": "npm run webdriver-manager start",
    "pree2e": "npm run webdriver:update -- --standalone",
    "e2e": "npm run protractor",
    "e2e:live": "npm run e2e -- --elementExplorer",
    "ci": "npm test && npm run e2e",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./Client/",
    "postinstall": "npm run typings -- install",
    "preversion": "npm test",
    "version": "npm run build",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.5",
    "@angular/compiler": "2.0.0-rc.5",
    "@angular/core": "2.0.0-rc.5",
    "@angular/http": "2.0.0-rc.5",
    "@angular/platform-browser": "2.0.0-rc.5",
    "@angular/forms": "0.3.0",
    "@angular/platform-browser-dynamic": "2.0.0-rc.5",
    "@angular/platform-server": "2.0.0-rc.5",
    "@angular/router": "3.0.0-rc.1",
    "aspnet-prerendering": "1.0.4",
    "aspnet-webpack": "1.0.9",
    "bootstrap": "3.3.7",
    "core-js": "2.4.1",
    "font-awesome": "4.6.3",
    "isomorphic-fetch": "2.2.1",
    "normalize.css": "4.2.0",
    "preboot": "2.0.10",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "angular2-template-loader": "^0.4.0",
    "awesome-typescript-loader": "1.1.1",
    "codelyzer": "0.0.26",
    "css": "2.2.1",
    "css-loader": "0.23.1",
    "es6-promise": "3.2.1",
    "es6-promise-loader": "1.0.1",
    "expose-loader": "0.7.1",
    "extendify": "1.0.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-loader": "^0.4.3",
    "istanbul-instrumenter-loader": "0.2.0",
    "json-loader": "0.5.4",
    "karma": "1.1.0",
    "karma-chrome-launcher": "1.0.1",
    "karma-coverage": "1.0.0",
    "karma-jasmine": "1.0.2",
    "karma-mocha-reporter": "2.0.4",
    "karma-phantomjs-launcher": "1.0.1",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.7.0",
    "node-sass": "3.8.0",
    "parse5": "2.1.5",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "2.1.7",
    "protractor": "3.3.0",
    "raw-loader": "0.5.1",
    "remap-istanbul": "^0.6.4",
    "rimraf": "2.5.2",
    "sass-loader": "4.0.0",
    "source-map-loader": "0.1.5",
    "style-loader": "0.13.1",
    "ts-helpers": "1.1.1",
    "ts-node": "0.9.3",
    "tslint": "3.12.1",
    "tslint-loader": "2.1.5",
    "typedoc": "0.4.4",
    "typescript": "^1.8.10",
    "url-loader": "0.5.7",
    "webpack": "1.13.1",
    "webpack-externals-plugin": "1.0.0",
    "webpack-hot-middleware": "2.12.0",
    "webpack-md5-hash": "0.0.5"
  }
}
